{
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "jsx": true,
    "modules": true,
    "templateStrings": true,
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "max-len": 0,
    "react/sort-comp": 0,
    "react/require-default-props": 0,
    "global-require": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": 0
  }
}